{
    "name": "elkady/activity-logger",
    "description": "A Laravel package for logging auths activities.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elkady\\ActivityLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elkady\\ActivityLogger\\ActivityLoggerServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
